				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#EVENThasVENUEID' type='key'>Venue:{venueid}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#EVENThasWORLD_PREMIER' type='string'>{world_premier}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#EVENThasYYYYFIRST_DATE' type='string'>{yyyyfirst_date}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#EVENThasYYYYLAST_DATE' type='string'>{yyyylast_date}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#EVENThasYYYYOPENING_NIGHT' type='string'>{yyyyopening_night}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#EVENThasYYYYDATE_ENTERED' type='string'>{yyyydate_entered}</object>
