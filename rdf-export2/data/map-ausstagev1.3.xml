			<subqueries>
				<subquery>
					<sql>SELECT * FROM eventworklink ORDER BY eventid</sql>
					<objects>
						<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#EVENThasWORKLINK' reversePredicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#WORKhasEVENTLINK' type='key'>Work:{workid}</object>
					</objects>
				</subquery>
			
				<subquery>
					<sql>select datasourceevlink.eventid eventid, datasource.datasource from datasourceevlink inner join datasource on datasourceevlink.datasourceid = datasource.datasourceid</sql>
					<objects>
						<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#EVENThasDATA_SOURCE' type='string'>{datasource}</object>
					</objects>
				</subquery>
				
				<subquery>
					<sql>select secgenreclasslink.eventid eventid, secgenrepreferred.preferredterm from secgenreclasslink inner join secgenrepreferred on secgenreclasslink.secgenrepreferredid = secgenrepreferred.secgenrepreferredid</sql>
					<objects>
						<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#EVENThasSECONDARYGENRE' type='string'>{preferredterm}</object>
					</objects>
				</subquery>
			</subqueries>
		</table>
		
				
