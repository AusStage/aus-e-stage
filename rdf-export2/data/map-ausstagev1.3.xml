		<table name='Organisation' type='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#ORGANISATION' primaryKey='organisationid'>
			<sql>SELECT organisation.*, country.countryname, organisation_type.Type type, states.state statename FROM organisation LEFT JOIN country on organisation.countryid = country.countryid LEFT JOIN states on organisation.state = states.stateid LEFT JOIN organisation_type on organisation.organisation_type_id = organisation_type.organisation_type_id</sql>
