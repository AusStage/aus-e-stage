<?xml version="1.0" encoding="UTF-8" ?>
<mapping>
	<namespaces>
		<namespace prefix='ausstage'>http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#</namespace>
	</namespaces>
	
	<tables>
	
		<table name='ContentIndicator' type='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTOR' primaryKey='contentindicatorid'>
			<sql>SELECT * FROM contentindicator</sql>
			<objects>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTENTINDICATORhasCONTENTINDICATOR' type='string'>{contentindicator}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTENTINDICATORhasCONTENTINDICATORDESCRIPTION' type='string'>{contentindicatordescription}</object>
			</objects>
		</table>
		
		<table name='ContFunc' type='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTFUNC' primaryKey='contributorfunctpreferredid'>
			<sql>SELECT * FROM contfunct</sql>
			<objects>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTFUNCThasCONTFUNCTION' type='string'>{contfunction}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTFUNCThasCONTFUNCTDESCRIPTION' type='string'>{contfunctiondescription}</object>
			</objects>
		</table>
		
		<table name='Contributor' type='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTOR' primaryKey='contributorid'>
			<sql>SELECT c.*, LOWER(g.gender) gendername, s.state statename FROM contributor c LEFT JOIN gendermenu g ON (c.gender = g.genderid) LEFT JOIN states s ON (s.stateid = c.state) ORDER BY contributorid</sql>
			<objects>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasCONTRIBUTORCOUNTRY' type='string'>{contributorcountry}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasDATE_OF_DEATH' type='dateTime'>{date_of_death}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasDDDATE_OF_DEATH' type='string'>{dddate_of_death}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasDISPLAY_NAME' type='string'>{first_name} {last_name}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasFIRST_NAME' type='string'>{first_name}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasGENDER' type='string' matches='male|female'>{gendername}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasLAST_NAME' type='string'>{last_name}</object>
				<!-- <object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasMIDDLE_NAME' type='string'>{middle_name}</object> -->
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasMMDATE_OF_DEATH' type='string'>{mmdate_of_death}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasNATIONALITY' type='string'>{nationality}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasNOTES' type='string'>{notes}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasOTHER_NAMES' type='string'>{other_names}</object>
				<!-- <object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasPLACE_OF_BIRTH' type='key'>Venue:{place_of_birth}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasPLACE_OF_DEATH' type='key'>Venue:{place_of_death}</object> -->
				<!-- <object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasPREFIX' type='string'>{prefix}</object> -->
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasSTATE' type='string' matches='[^U].*'>{statename}</object>
				<!-- <object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasSUFFIX' type='string'>{suffix}</object> -->
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasYYYYDATE_OF_DEATH' type='string'>{yyyydate_of_death}</object>
				
				
			</objects>
			
			<subqueries>
				<subquery>
					<sql>SELECT * FROM contfunctlink ORDER BY contributorid</sql>
					<objects>
						<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasCONTFUNCT' type='key'>ContFunc:{contributorfunctpreferredid}</object>
					</objects>
				</subquery>
				
				<!--
				<subquery>
					<sql>SELECT contribnationlink.contributorid contributorid, country.countryname countryname FROM contribnationlink INNER JOIN country ON contribnationlink.countryid = country.countryid ORDER BY contribnationlink.contributorid</sql>
					<objects>
						<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasNATIONLINK' type='string'>{countryname}</object>
					</objects>
				</subquery>
				-->
				
				<subquery>
					<sql>SELECT * FROM conorglink ORDER BY contributorid</sql>
					<objects>
						<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasORGANISATIONLINK' type='key'>Organisation:{organisationid}</object>
					</objects>
				</subquery>
				
				<subquery>
					<sql><![CDATA[SELECT distinct a.contributorid contributorid, b.contributorid collaboratorid FROM conevlink a INNER JOIN conevlink b ON a.eventid = b.eventid and a.contributorid < b.contributorid order by a.contributorid]]></sql>
					<objects>
						<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasCONTRIBUTORLINK' type='key' reversePredicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#CONTRIBUTORhasCONTRIBUTORLINK'>Contributor:{collaboratorid}</object>
					</objects>
				</subquery>
			</subqueries>
		</table>
		
		<table name='DescriptionSrc' type='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#DESCRIPTIONSRC' primaryKey='descriptionsourceid'>
			<sql>SELECT * FROM descriptionsrc</sql>
			<objects>
