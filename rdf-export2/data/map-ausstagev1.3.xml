					<sql><![CDATA[SELECT o.organisationid, date_format(min(e.first_date), "%d") ddfirst, date_format(max(e.last_date), "%d") ddlast, date_format(min(e.first_date), "%m") mmfirst, date_format(max(e.last_date), "%m") mmlast, date_format(min(e.first_date), "%Y") yyyyfirst, date_format(max(e.last_date), "%Y") yyyylast, min(e.first_date) firstdate, max(e.last_date) lastdate FROM organisation o INNER JOIN orgevlink oe ON (o.organisationid = oe.organisationid and o.organisationid IS NOT NULL) INNER JOIN events e ON (oe.eventid = e.eventid) GROUP BY o.organisationid]]></sql>
					<objects>
