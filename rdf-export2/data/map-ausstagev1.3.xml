				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#ITEMhasDETAIL_COMMENTS' type='string'>{detail_comments}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#ITEMhasDONATED_PURCHASED' type='string'>{donated_purchased}</object>
