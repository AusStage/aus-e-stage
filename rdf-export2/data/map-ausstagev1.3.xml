				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#EVENThasSNDFIRST_DATE' type='nonNegativeInteger'>{sndfirst_date}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#EVENThasSNDLAST_DATE' type='nonNegativeInteger'>{sndlast_date}</object>
				<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#EVENThasSNDOPENING_NIGHT' type='nonNegativeInteger'>{sndopening_night}</object>
