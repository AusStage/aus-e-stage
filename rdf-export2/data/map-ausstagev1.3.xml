					<objects>
						<object predicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#ORGANISATIONhasEVLINK' reversePredicate='http://www.semanticweb.org/ontologies/2011/6/AusStagev1-3.owl#EVENThasORGANISATIONLINK' type='key'>Event:{eventid}</object>
